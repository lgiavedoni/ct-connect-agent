{
  "name": "service-typescript",
  "description": "",
  "version": "1.0.0",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "node build/index.js",
    "gcp-build": "tsc",
    "start:dev": "concurrently -k \"tsc --watch\" \"nodemon -q build/index.js\"",
    "debug": "concurrently -k \"tsc --watch\" \"nodemon --inspect=9237 -q build/index.js\"",
    "debug:ts": "node --inspect=9236 -r ts-node/register src/index.ts",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "debug:dev": "nodemon --watch 'src/**/*.ts' --exec 'node --inspect=9235 -r ts-node/register' src/index.ts",
    "debug:nodemon": "cross-env PORT=3005 nodemon --watch 'src/**/*.ts' --exec 'node --inspect=9234 -r ts-node/register' src/index.ts",
    "build": "rimraf ./build && tsc",
    "lint": "eslint . --ext .ts",
    "prettier": "prettier --write '**/*.{js,ts}'",
    "test": "jest --config jest.config.cjs",
    "test:watch": "jest --watch",
    "connector:post-deploy": "node build/connector/post-deploy.js",
    "connector:pre-undeploy": "node build/connector/pre-undeploy.js",
    "kill:ports": "kill-port 3000 3001 3002 3003 3004 3005 9229 9230 9231 9232 9233 9234 9235 9236 9237 || true",
    "debug:clean": "npm run kill:ports && npm run debug:vscode",
    "debug:vscode": "cross-env PORT=3005 TS_NODE_FILES=true nodemon --watch 'src/**/*.ts' --exec 'node --inspect=127.0.0.1:9234 -r ts-node/register --preserve-symlinks --enable-source-maps' src/index.ts"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.0.3",
    "@types/node": "^18.7.18",
    "@types/validator": "^13.7.10",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "concurrently": "^8.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "kill-port": "^2.0.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.13",
    "@ai-sdk/google": "^1.1.17",
    "@ai-sdk/openai": "^1.2.0",
    "@commercetools-backend/loggers": "^21.19.0",
    "@commercetools/platform-sdk": "^4.0.0",
    "@commercetools/sdk-client-v2": "^2.0.0",
    "@types/cors": "^2.8.17",
    "ai": "^4.1.46",
    "axios": "^1.8.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "graphql-request": "^7.1.2",
    "helmet": "^8.0.0",
    "validator": "^13.11.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
